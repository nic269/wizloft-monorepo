{
	"name": "@wizloft/database",
	"author": "Anh Nguyen",
	"license": "MIT",
	"description": "Wizloft database library",
	"keywords": [
		"database",
		"library",
		"prisma"
	],
	"version": "0.1.0",
	"main": "./dist/client.js",
	"types": "./dist/client.d.ts",
	"exports": {
		".": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.js",
			"require": "./dist/client.js"
		}
	},
	"scripts": {
		"db:migrate:deploy": "prisma migrate deploy",
		"db:migrate:dev": "prisma migrate dev",
		"db:push": "prisma db push",
		"db:seed": "tsx src/seed.ts",
		"build": "tsup && tsc --emitDeclarationOnly --declaration",
		"dev": "tsup --watch & tsc --emitDeclarationOnly --declaration --watch",
		"format": "prisma format",
		"generate": "prisma generate",
		"lint": "biome check --write",
		"prebuild": "npm run generate",
		"predev": "npm run generate",
		"studio": "prisma studio"
	},
	"dependencies": {
		"@prisma/adapter-pg": "^7.2.0",
		"@prisma/client": "^7.2.0",
		"pg": "^8.16.3"
	},
	"devDependencies": {
		"@prisma/client-runtime-utils": "^7.2.0",
		"@types/node": "^25.0.3",
		"@types/pg": "^8.16.0",
		"@wizloft/biome-config": "workspace:*",
		"@wizloft/typescript-config": "workspace:*",
		"dotenv": "^17.2.3",
		"prisma": "^7.2.0",
		"rimraf": "^6.1.2",
		"tsup": "^8.5.1",
		"tsx": "4.21.0",
		"typescript": "5.9.3"
	}
}
